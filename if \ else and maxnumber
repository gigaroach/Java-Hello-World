import java.util.Scanner;

class MyProg {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
        int maxnum;
        int a = scan.nextInt();
        int b = scan.nextInt();
        int c = scan.nextInt();
        scan.close();
        if (a >= b) {
            maxnum = a;
        } else {
        maxnum = b;
        } if (c > b) {
            maxnum = c;
        }
        System.out.println(maxnum);
	}
}
